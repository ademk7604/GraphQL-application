scalar DateTime



type Query {
    getAllYSers: [User]
    getById(id: ID!): User

}

type Mutation{
    createUser(userRequest: UserRequest!): User
    updateUser(userRequest: UserRequest!): User
    deleteUser(id:ID!) : Boolean

}

type User {
    id: ID
    created: LocaleDate
    updated: LocaleDate
    username: String
    mail: String
    role:Role
}

enum Role {
    ADMIN
    USER
}

input UserRequest {
    id: ID
    username: String
    mail: String
    role:Role
}